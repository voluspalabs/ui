{
  "name": "@voluspalabs/ui",
  "version": "0.0.5",
  "description": "A collection of high-quality React components styled with Tailwind CSS for building modern web interfaces.",
  "keywords": ["UI", "typescript", "react", "tailwindcss"],
  "homepage": "https://github.com/voluspa-labs/ui",
  "bugs": {
    "url": "https://github.com/voluspa-labs/ui/issues"
  },
  "author": "Voluspa <info@voluspa.io> (https://voluspa.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voluspa-labs/ui.git"
  },
  "files": ["dist"],
  "type": "module",
  "license": "BSL-1.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "bunx del-cli ./dist",
    "build": "tsup",
    "ci": "bun run build && bun run biome ci && bun run check-exports",
    "check-format": "biome check --write",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm --profile esm-only",
    "local-release": "bun run ci && changeset version && changeset publish"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.0.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
